{{#each main}}
	<div class="card defaultCard">
		<a href="{{url}}" class="aCard"> 
			<div class="card-body">
			<div class="cheader">
				<h2> {{name}} </h2>
			</div>

				{{!-- ----------/ --}}

				<div class="card_image">
					<img src="{{path}}" alt="Нема картинк ? Тисни на блок це є кнопка" class="img">
				</div>
				<div class="cfooter">
					<span class="card_price_duration"> {{value}} </span>
				</div>
			</div>
		</a>
	</div>
{{/each}}
